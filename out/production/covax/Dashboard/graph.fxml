<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: D0E1F9;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Dashboard.Graph">
   <children>
      <Button fx:id="graphconfirmed" layoutX="313.0" layoutY="126.0" mnemonicParsing="false" onAction="#graphConfirmed" style="-fx-background-color: #4D648D;" text="Confirmed Cases Graph " textFill="WHITE">
         <font>
            <Font name="HP Simplified Light" size="16.0" />
         </font></Button>
      <Button fx:id="graphrecovered" layoutX="314.0" layoutY="192.0" mnemonicParsing="false" onAction="#graphRecovered" style="-fx-background-color: #4D648D;" text="Recovered Cases Graph" textFill="WHITE">
         <font>
            <Font name="HP Simplified Light" size="16.0" />
         </font></Button>
      <Button fx:id="graphdeath" layoutX="317.0" layoutY="254.0" mnemonicParsing="false" onAction="#graphDeath" style="-fx-background-color: #4D648D;" text="Deceased Cases Graph" textFill="WHITE">
         <font>
            <Font name="HP Simplified Light" size="16.0" />
         </font></Button>
      <Button fx:id="updatebydate" layoutX="341.0" layoutY="315.0" mnemonicParsing="false" onAction="#updatebyDate" style="-fx-background-color: #4D648D;" text="Search By Date" textFill="WHITE">
