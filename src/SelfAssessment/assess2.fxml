            <RadioButton fx:id="q3yes" layoutX="50.0" layoutY="393.0" mnemonicParsing="false" text="Yes">
               <font>
                  <Font name="HP Simplified Light" size="14.0" />
               </font></RadioButton>
            <RadioButton fx:id="q3no" layoutX="51.0" layoutY="420.0" mnemonicParsing="false" text="No">
