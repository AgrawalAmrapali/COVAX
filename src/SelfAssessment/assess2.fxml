                    <Insets right="10.0" />
                </padding>
               <font>
                  <Font name="HP Simplified Light" size="14.0" />
               </font></CheckBox>
            <CheckBox fx:id="q2lung" layoutX="48.0" layoutY="217.0" mnemonicParsing="false" text="Lung disease">
