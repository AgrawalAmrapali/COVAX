<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SelfAssessment.Assess2">
<columnConstraints>
    <ColumnConstraints />
